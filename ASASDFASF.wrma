PARA oopify(prefix) -> prefix + "oop"

PARA join(elements, separator)
    DEF result = "";
    DEF len = LEN(elements);

    FOR i = 0 TO len THEN
        DEF result = result + elements/i;
        IF i != len - 1 THEN DEF result = result + separator;
    FIN

    RETURN result;
FIN